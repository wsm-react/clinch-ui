{
  "name": "clinch-ui",
  "version": "0.1.0",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wsm-react/clinch-ui.git"
  },
  "bugs": {
    "url": "https://github.com/wsm-react/clinch-ui/issues"
  },
  "homepage": "https://github.com/wsm-react/clinch-ui#readme",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "yarn cache clean && rm -rf node_modules && rm -rf globe-report-admin && rm -rf yarn.lock && rm -rf package-lock.json && yarn globe-report-admin && yarn start ",
    "git": "git add . && git commit -m \"$COMMENT :: on $(date +'%a, %b %d, %Y %r')\" && git push --all --force",
    "prisma:generate": "npx dotenv -e .env.local -- prisma generate",
    "migrate:dev": "npx dotenv -e .env.local -- prisma migrate dev",
    "prisma:studio": "npx dotenv -e .env.local -- prisma studio",
    "migrate": "npx dotenv -e .env.local -- prisma migrate deploy",
    "build:production": "npx dotenv -e .env.local -- npm run migrate && npm run generate && npm run build"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.5.0",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-radio-group": "^1.1.3",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tanstack/react-table": "^8.17.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "input-otp": "^1.2.4",
    "lucide-react": "^0.383.0",
    "next": "14.2.3",
    "react": "^18",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18",
    "react-hook-form": "^7.51.5",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/tailwindcss": "^3.1.0",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "typescript": "^5"
  }
}
